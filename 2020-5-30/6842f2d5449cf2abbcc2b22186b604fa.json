      filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#64a7ce', endColorstr='#38698c');
      color: #fff;
      border: none;
      height:20px;
    }

    .paneContent {
      border-top: none;
      background-color: transparent;
      padding: 4px 38px 20px;
    }

    .radioHolder{
      padding:0 0 16px 48px;
    }

    .configDialog {
      width: 728px;
      font-size: 1em
